{
    "name": "heimrichhannot/contao-exporter-bundle",
    "description":"A backend module for exporting any contao entity to file.",
    "keywords":["contao", "export", "backend", "bundle", "developer", "xls", "csv"],
    "type": "contao-bundle",
    "homepage":"https://github.com/heimrichhannot/contao-exporter-bundle",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Heimrich & Hannot",
            "email":"digitales@heimrich-hannot.de",
            "homepage":"https://www.heimrich-hannot.de",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/heimrichhannot/contao-exporter-bundle/issues"
    },
    "require": {
        "php": "~7.1",
        "contao/core-bundle": "^4.1",
        "phpoffice/phpspreadsheet": "^1.3",
        "heimrichhannot/contao-utils-bundle": "^2.25",
        "heimrichhannot/contao-request-bundle": "^1.0",
        "heimrichhannot/contao-haste_plus": ">=1.3.23",
        "heimrichhannot/contao-fieldpalette": ">=1.2"
    },
    "require-dev": {
      "contao/core-bundle":"4.4.*"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\ContaoExporterBundle\\": "src/"
        },
        "classmap": [
            "src/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\ContaoExporterBundle\\ContaoManager\\Plugin"
    }
}
